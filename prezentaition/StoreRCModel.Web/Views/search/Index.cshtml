
@model StoreRCModels.RCModel[];

@{
    ViewData["Title"] = "Результаты поиска";
}

<h1>Результаты поиска</h1>

@if (Model.Length == 0)
{
    <p> Ничего не найдено.</p>
}
else
{
    <ul>@foreach(var rcmodel in Model)
        {
            <li><a href="@Url.Action("Index","RCModel", new{id = rcmodel.numberRCModel })"> @rcmodel.nameRCModel</a></li>
        }</ul>
}

